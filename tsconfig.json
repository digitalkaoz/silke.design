{
  "compilerOptions": {
    "target": "es2024",
    "module": "es6",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "types": ["vite/client"],
    "paths": {}
  },
  "include": ["./src/**/*", "./src/App.jsx", "./src/main.jsx"],
  "exclude": ["node_modules", "dist", "build"]
}
